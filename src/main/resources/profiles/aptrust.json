{
   "BagIt-Profile-Info":{
      "BagIt-Profile-Identifier":"http://fedora.info/bagprofile/aptrust.json",
      "BagIt-Profile-Version":"1.3.0",
      "Source-Organization":"Duraspace",
      "Contact-Name":"Esm√© Cowles",
      "Contact-Email":"escowles@ticklefish.org",
      "External-Description":"Bag Profile for Fedora and APTrust.",
      "Version":"2.2"
   },
   "Bag-Info":{
      "Source-Organization":{
         "required":true
      },
      "Organization-Address":{
         "required":false
      },
      "Contact-Name":{
         "required":false
      },
      "Contact-Phone":{
         "required":false
      },
      "Contact-Email":{
         "required":false
      },
      "External-Description":{
         "required":false
      },
      "External-Identifier":{
         "required":false
      },
      "Bag-Size":{
         "required":true
      },
      "Bag-Group-Identifier":{
         "required":false
      },
      "Bag-Count":{
         "required":false
      },
      "Internal-Sender-Identifier":{
         "required":false
      },
      "Internal-Sender-Description":{
         "required":false
      },
      "Bagging-Date":{
         "required":true
      },
      "Payload-Oxum":{
         "required":true
      }
   },
   "Other-Info" : [
      {"APTrust-Info":{
         "Title":{
            "required":true
         },
         "Access":{
            "required":true,
            "values":["Consortia","Institution","Restricted"]
         },
         "Storage-Option": {
            "required": true,
            "values": [
               "Standard",
               "Glacier-OH",
               "Glacier-OR",
               "Glacier-VA",
               "Glacier-Deep-OH",
               "Glacier-Deep-OR",
               "Glacier-Deep-VA"
            ]
         }
      }}
   ],
   "Manifests-Required":[
      "md5"
   ],
   "Manifests-Allowed": [
      "md5", "sha256"
   ],
   "Allow-Fetch.txt":false,
   "Serialization":"optional",
   "Accept-Serialization":[
      "application/tar"
   ],
   "Tag-Files-Required":[
     "aptrust-info.txt"
   ],
   "Tag-Manifests-Required":[
     "md5"
   ],
   "Tag-Manifests-Allowed":[
     "md5", "sha256"
   ],
   "Accept-BagIt-Version":[
      "0.97",
      "1.0"
   ]
}
